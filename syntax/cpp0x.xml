<language name="C++" section="Sources"
          version="1.49.8" kateversion="2.4"
          indenter="cstyle"
          mimetype="text/x-c++src;text/x-c++hdr;text/x-chdr"
          extensions="*.c++;*.cxx;*.cpp;*.cc;*.C;*.h;*.hh;*.H;*.h++;*.hxx;*.hpp;*.hcc;*.moc"
          author="Alex Turbov (i.zaufi@gmail.com)"
          priority="20">
